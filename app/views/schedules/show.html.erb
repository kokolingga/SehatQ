<h1>Doctor Booking</h1>

<p>
  <strong>Name:</strong>
  <%= @doctor.name %>
</p>

<p>
  <strong>Hospital:</strong>
  <%= @doctor.hospital.name %>
</p>

<table class="table table-striped table-responsive-md btn-table">
  <thead>
    <tr>
      <th>Schedule ID</th>
      <th>Start</th>
      <th>End</th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td><%= @schedule.id %></td>
        <td><%= @schedule.date_start %></td>
        <td><%= @schedule.date_end %></td>
      </tr>
  </tbody>
</table>


<%= render 'booking', booking: @booking %>